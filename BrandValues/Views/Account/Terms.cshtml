@{
    ViewBag.Title = "Terms & Conditions";
}

<div class="row">
    <div class="col-md-10 col-md-offset-1">
        <section id="loginForm">

            <h1 class="largest-text purple">Terms & Conditions</h1>
            <hr />

        </section>



        <p class="text-center centered">
            @Html.ActionLink("Close", null, null, new { @class = "btn btn-grey btn-block rounded", id = "close" })
        </p>

        <script>
            $("#close").click(function () { window.close(); });
        </script>

    </div>
    

</div>
