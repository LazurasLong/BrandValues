@model IEnumerable<BrandValues.Models.SiteVersion>

    @{
        ViewBag.Title = "Index";
    }

    <section class="vbox">
<section class="scrollable padder">

    <h2>Site Admin Overview</h2>

    @*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@
    <table class="table">
        <tr>
            <th>
               @Html.DisplayNameFor(model => model.Homepage)
            </th>
            <th>

            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    Current Version: @Html.DisplayFor(modelItem => item.Homepage)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) 
                    @*@Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
                </td>
            </tr>
        }

    </table>
</section>
</section>