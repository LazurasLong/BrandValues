                                            <span class="accordion-icon"><i class="fa icon-cloud-upload time-icon bg-primary"></i></span>
                                            <span class="timeline-date white">Click here to enter</span>
                                            <i class="icon-arrow-right white i-lg pull-right acc-icon-padding"></i>
                                        </div>
                                    </a>
                                </div>
                                <div class="panel panel-default no-top-margin no-border-radius no-border">
                                    <a data-ajax="true" data-ajax-mode="replace" data-ajax-update="#result" href="~/Home/WinningEntry">
                                        <div class="panel-heading menu-panel">
                                            <span class="accordion-icon"><i class="fa icon-eye time-icon bg-primary"></i></span>
                                            <span class="timeline-date white">What does a winning entry look like?</span>
                                            <i class="icon-arrow-right white i-lg pull-right acc-icon-padding"></i>
                                        </div>
                                    </a>
                                </div>
                                <div class="panel panel-default no-top-margin no-border-radius no-border">
                                    <a href="~/Home/faq">
                                        <div class="panel-heading menu-panel">
                                            <span class="accordion-icon"><i class="fa icon-question time-icon bg-primary"></i></span>
                                            <span class="timeline-date white">All your questions answered… </span>
                                            <i class="icon-arrow-right white i-lg pull-right acc-icon-padding"></i>
                                        </div>
                                    </a>
                                </div>

                            </div>
