<!DOCTYPE html>
<html lang="en" class="app">
<head>
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="black" name="apple-mobile-web-app-status-bar-style">

    <title>@ViewBag.Title</title>

    <link href="~/Content/webapp/images/icons/favicon.ico" rel="shortcut icon" type="image/x-icon">
    <link href="~/Content/webapp/images/icons/iphone.jpg" rel="apple-touch-icon">
    <link href="~/Content/webapp/images/icons/ipad.jpg" rel="apple-touch-icon" sizes="72x72">
    <link href="~/Content/webapp/images/icons/iphone4.jpg" rel="apple-touch-icon" sizes="114x114">
    <link href="~/Content/webapp/images/icons/ipad3.jpg" rel="apple-touch-icon" sizes="144x144">

    @Styles.Render("~/Bundles/css")

    @*<link rel="stylesheet" href="~/Content/webapp/css/bootstrap.css" type="text/css" />
        <link rel="stylesheet" href="~/Content/webapp/css/animate.css" type="text/css" />
        <link rel="stylesheet" href="~/Content/webapp/css/font-awesome.min.css" type="text/css" />
        <link rel="stylesheet" href="~/Content/webapp/css/simple-line-icons.css" type="text/css" />
        <link rel="stylesheet" href="~/Content/webapp/css/font.css" type="text/css" />
        <link rel="stylesheet" href="~/Content/webapp/less/app.css" type="text/css" />*@
   

    <!--[if lt IE 9]>
      <script src="https://d3kx2j4tswsg1z.cloudfront.net/ie/html5shiv.js"></script>
      <script src="https://d3kx2j4tswsg1z.cloudfront.net/ie/respond.min.js"></script>
      <script src="https://d3kx2j4tswsg1z.cloudfront.net/ie/excanvas.js"></script>
    <![endif]-->
    
    <script src="~/Scripts/webapp/jquery.min.js"></script> 
   
</head>
<body class="">

    <!-- Body content -->
    <section class="vbox">

        <header class="bg-white-only header header-md navbar navbar-fixed-top-xs">
            <div class="navbar-header aside brand-white">
                <a class="btn btn-link visible-xs" data-toggle="class:nav-off-screen,open" data-target="#nav,html">
                    <i class="icon-list"></i>
                </a>
                <a href="~/home/" class="navbar-brand text-dark">
                    <img src="~/images/aib-logo-small.png" alt="AIB Logo">
                    <span class="hidden-nav-xs m-l-sm">Brand Values</span>
                </a>
                <a class="btn btn-link visible-xs" data-toggle="dropdown" data-target=".user">
                    <i class="icon-settings"></i>
                </a>
            </div>      <ul class="nav navbar-nav hidden-xs">
                            <li>
                                <a href="#nav,.navbar-header" data-toggle="class:nav-xs,nav-xs" class="text-muted">
                                    <i class="fa fa-indent text"></i>
                                    <i class="fa fa-dedent text-active"></i>
                                </a>
                            </li>
                        </ul>
            <form class="navbar-form navbar-left input-s-lg m-t m-l-n-xs hidden-xs hidden-tablet" role="search">
                <div class="form-group">
                    <div class="input-group">
                        <span class="input-group-btn">
                            <button type="submit" class="btn btn-sm bg-white btn-icon rounded"><i class="fa fa-search"></i></button>
                        </span>
                        <input type="text" class="form-control input-sm no-border rounded" placeholder="Search...">
                    </div>
                </div>
            </form>
            <div class="navbar-right ">
                <ul class="nav navbar-nav m-n hidden-xs nav-user user">
                    <li class="hidden-xs">
                        <a href="#" class="dropdown-toggle lt" data-toggle="dropdown">
                            <i class="icon-bell"></i>
                            <span class="badge badge-sm up bg-danger count">1</span>
                        </a>
                        <section class="dropdown-menu aside-xl animated fadeInUp">
                            <section class="panel bg-white">
                                <div class="panel-heading b-light bg-light">
                                    <strong>You have <span class="count">1</span> notifications</strong>
                                </div>
                                <div class="list-group list-group-alt">
                                    <a href="~/home/upload" class="media list-group-item">
                                        <span class="media-body block m-b-none">
                                            The competition is open! Get your entry in<br>
                                            <small class="text-muted">27th Aug</small>
                                        </span>
                                    </a>
                                </div>
                                <div class="panel-footer text-sm">
                                    <a href="#" class="pull-right"><i class="fa fa-cog"></i></a>
                                    <a href="#notes" data-toggle="class:show animated fadeInRight">See all the notifications</a>
                                </div>
                            </section>
                        </section>
                    </li>
                    
                    @if (Request.IsAuthenticated && User.IsInRole("Admin"))
                    {

                    <li class="dropdown">

                        <a href="#" class="dropdown-toggle bg clear" data-toggle="dropdown">
                            Admin <b class="caret"></b>
                        </a>

                        <ul class="dropdown-menu animated fadeInRight">
                            
                            <li>@Html.ActionLink("Site Admin", "Index", "SiteAdmin")</li>
                            
                            <li>@Html.ActionLink("Roles Admin", "Index", "RolesAdmin")</li>

                            <li>@Html.ActionLink("Users Admin", "Index", "UsersAdmin")</li>
                            
                        </ul>
                            
                           

                    </li>
                    }
                    
                    
                    @Html.Partial("_LoginPartial")

                </ul>
            </div>
        </header>


        <section>
            <section class="hbox stretch">
                <!-- .aside -->
                <aside class="bg-black dk aside hidden-print" id="nav">
                    <section class="vbox">
                        <section class="w-f-md scrollable">
                            <div class="slimScrollDiv" style="position: relative; overflow: hidden; width: auto; height: 430px;">
                                <div class="slim-scroll" data-height="auto" data-disable-fade-out="true" data-distance="0" data-size="10px" data-railopacity="0.2" style="overflow: hidden; width: auto; height: 430px;">
                                    <!-- nav -->
                                    <nav class="nav-primary hidden-xs">
                                        <ul class="nav bg clearfix">
                                            <li class="hidden-nav-xs padder m-t m-b-sm text-xs text-muted">
                                                Discover
                                            </li>
                                            <li>
                                                <a href="~/home">
                                                    <i class="icon-like icon"></i>

                                                    <span>Discover</span>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="~/home/browse">
                                                    <i class="icon-grid icon"></i>
                                                    <span>Browse</span>
                                                </a>
                                            </li>
                                            <li class="hidden-nav-xs padder m-t m-b-sm text-xs text-muted">
                                                Your Entry
                                            </li>
                                            <li>
                                                <a href="~/home/upload">
                                                    <i class="icon-cloud-upload icon  "></i>
                                                    <span>Upload</span>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="~/home/entry">
                                                    <i class="icon-pencil icon"></i>
                                                    <span>Edit</span>
                                                </a>
                                            </li>

                                            <li class="hidden-nav-xs padder m-t m-b-sm text-xs text-muted">
                                                Help
                                            </li>
                                            <li>
                                                <a href="~/home/faq">
                                                    <i class="icon-info" icon="" "=" "></i>
                                                    <span>FAQ's</span>
                                                </a>
                                            </li>


                                            <li class="hidden-nav-xs padder m-t m-b-sm text-xs text-muted">
                                                Admin
                                            </li>
                                            <li>
                                                <a href="~/SiteAdmin">
                                                    <i class="icon-settings icon"></i>
                                                    <span>Site Admin</span>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="~/UsersAdmin">
                                                    <i class="icon-users icon"></i>
                                                    <span>Users Admin</span>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="~/RolesAdmin">
                                                    <i class="icon-magic-wand icon"></i>
                                                    <span>Roles Admin</span>
                                                </a>
                                            </li>

                                            <li class="m-b hidden-nav-xs"></li>
                                        </ul>

                                    </nav>
                                    <!-- / nav -->
                                </div><div class="slimScrollBar" style="width: 10px; position: absolute; top: -330px; opacity: 0.4; display: none; border-top-left-radius: 7px; border-top-right-radius: 7px; border-bottom-right-radius: 7px; border-bottom-left-radius: 7px; z-index: 99; right: 0px; height: 389.2631578947369px; background: rgb(0, 0, 0);"></div><div class="slimScrollRail" style="width: 10px; height: 100%; position: absolute; top: 0px; display: none; border-top-left-radius: 7px; border-top-right-radius: 7px; border-bottom-right-radius: 7px; border-bottom-left-radius: 7px; opacity: 0.2; z-index: 90; right: 0px; background: rgb(51, 51, 51);"></div>
                            </div>
                        </section>

                        <footer class="footer hidden-xs no-padder text-center-nav-xs"></footer>
                    </section>
                </aside>
                

               
                <!-- /.aside -->
                <section id="content">
                    
                    <section class="hbox stretch">

                        @RenderBody()
                    </section>

                        <a href="#" class="hide nav-off-screen-block" data-toggle="class:nav-off-screen" data-target="#nav"></a>
                    </section>
            </section>
        </section>

    </section>
                <!-- /Body content -->
    
    @Scripts.Render("~/Bundles/webapp")
                
                @*<!-- Bootstrap -->
                <script src="~/Scripts/webapp/bootstrap.min.js"></script>
                <!-- App -->
                <script src="~/Scripts/webapp/app.min.js"></script>
                <script src="~/Scripts/webapp/slimscroll/jquery.slimscroll.min.js"></script>
                <script src="~/Scripts/webapp/app.plugin.js"></script>
   
                <script src="~/Scripts/webapp/jwplayer/jwplayer.js"></script>
                <script src="~/Scripts/webapp/jwplayer/jwplayer.html5.js"></script>
    
                <!-- Upload -->
                <script type="text/javascript" src="@Url.Content("~/Scripts/webapp/parsley/parsley.min.js")"></script>
                <script type="text/javascript" src="~/Scripts/webapp/wizard/jquery.bootstrap.wizard.js"></script>*@
                

    @RenderSection("scripts", required: false)
                 
                

    
