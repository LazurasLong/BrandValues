<!DOCTYPE html>
<html lang="en" class="app">
<head>
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="black" name="apple-mobile-web-app-status-bar-style">

    <title>@ViewBag.Title</title>

    <link href="~/Content/webapp/images/icons/favicon.ico" rel="shortcut icon" type="image/x-icon">
    <link href="~/Content/webapp/images/icons/iphone.jpg" rel="apple-touch-icon">
    <link href="~/Content/webapp/images/icons/ipad.jpg" rel="apple-touch-icon" sizes="72x72">
    <link href="~/Content/webapp/images/icons/iphone4.jpg" rel="apple-touch-icon" sizes="114x114">
    <link href="~/Content/webapp/images/icons/ipad3.jpg" rel="apple-touch-icon" sizes="144x144">

    <link rel="stylesheet" href="~/Scripts/webapp/jPlayer/jplayer.flat.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/webapp/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/webapp/css/animate.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/webapp/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/webapp/css/simple-line-icons.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/webapp/css/font.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/webapp/less/app.css" type="text/css" />

    <!--[if lt IE 9]>
      <script src="Scripts/webapp/ie/html5shiv.js"></script>
      <script src="Scripts/webapp/ie/respond.min.js"></script>
      <script src="Scripts/webapp/ie/excanvas.js"></script>
    <![endif]-->
</head>
<body class="">

    <!-- Body content -->
    <section class="vbox">

        <header class="bg-white-only header header-md navbar navbar-fixed-top-xs">
            <div class="navbar-header aside brand-white nav-xs">
                <a class="btn btn-link visible-xs" data-toggle="class:nav-off-screen,open" data-target="#nav,html">
                    <i class="icon-list"></i>
                </a>
                <a href="https://valuescompetition.com" class="navbar-brand text-dark">
                    <img src="~/Content/webapp/images/aib-logo-small.png" alt="AIB Logo">
                    <span class="hidden-nav-xs m-l-sm">Brand Values</span>
                </a>
                <a class="btn btn-link visible-xs" data-toggle="dropdown" data-target=".user">
                    <i class="icon-settings"></i>
                </a>
            </div>      <ul class="nav navbar-nav hidden-xs">
                            <li>
                                <a href="#nav,.navbar-header" data-toggle="class:nav-xs,nav-xs" class="text-muted">
                                    <i class="fa fa-indent text"></i>
                                    <i class="fa fa-dedent text-active"></i>
                                </a>
                            </li>
                        </ul>
            <form class="navbar-form navbar-left input-s-lg m-t m-l-n-xs hidden-xs" role="search">
                <div class="form-group">
                    <div class="input-group">
                        <span class="input-group-btn">
                            <button type="submit" class="btn btn-sm bg-white btn-icon rounded"><i class="fa fa-search"></i></button>
                        </span>
                        <input type="text" class="form-control input-sm no-border rounded" placeholder="Search...">
                    </div>
                </div>
            </form>
            <div class="navbar-right ">
                <ul class="nav navbar-nav m-n hidden-xs nav-user user">
                    <li class="hidden-xs">
                        <a href="#" class="dropdown-toggle lt" data-toggle="dropdown">
                            <i class="icon-bell"></i>
                            <span class="badge badge-sm up bg-danger count">2</span>
                        </a>
                        <section class="dropdown-menu aside-xl animated fadeInUp">
                            <section class="panel bg-white">
                                <div class="panel-heading b-light bg-light">
                                    <strong>You have <span class="count">2</span> notifications</strong>
                                </div>
                                <div class="list-group list-group-alt">
                                    <a href="#" class="media list-group-item">
                                        <span class="pull-left thumb-sm">
                                            <img src="~/Content/webapp/images/a0.png" alt="..." class="img-circle">
                                        </span>
                                        <span class="media-body block m-b-none">
                                            Use awesome animate.css<br>
                                            <small class="text-muted">10 minutes ago</small>
                                        </span>
                                    </a>
                                    <a href="#" class="media list-group-item">
                                        <span class="media-body block m-b-none">
                                            1.0 initial released<br>
                                            <small class="text-muted">1 hour ago</small>
                                        </span>
                                    </a>
                                </div>
                                <div class="panel-footer text-sm">
                                    <a href="#" class="pull-right"><i class="fa fa-cog"></i></a>
                                    <a href="#notes" data-toggle="class:show animated fadeInRight">See all the notifications</a>
                                </div>
                            </section>
                        </section>
                    </li>
                    <li class="dropdown">
                        <ul class="nav navbar-nav navbar-right">
                            @if (Request.IsAuthenticated && User.IsInRole("Admin"))
                            {

                                <li>@Html.ActionLink("Roles Admin", "Index", "RolesAdmin")</li>
                            
                                <li>@Html.ActionLink("Users Admin", "Index", "UsersAdmin")</li>
                            }
</ul>
                    </li>
                    <li class="dropdown">
                        @Html.Partial("_LoginPartial")
                    </li>
                </ul>
            </div>
        </header>


        <section>
            <section class="hbox stretch">
                <!-- .aside -->
                <aside class="bg-black dk nav-xs aside hidden-print" id="nav">
                    <section class="vbox">
                        <section class="w-f-md scrollable">
                            <div class="slim-scroll" data-height="auto" data-disable-fade-out="true" data-distance="0" data-size="10px" data-railopacity="0.2">
                                <!-- nav -->
                                <nav class="nav-primary hidden-xs">
                                    <ul class="nav bg clearfix">
                                        <li class="hidden-nav-xs padder m-t m-b-sm text-xs text-muted">
                                            Discover
                                        </li>
                                        <li>
                                            <a ui-sref="main">
                                                <i class="icon-like icon"></i>
                                                <b class="badge bg-primary pull-right">1</b>
                                                <span>Discover</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a ui-sref="browse">
                                                <i class="icon-grid icon"></i>
                                                <span>Browse</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a ui-sref="upload">
                                                <i class="icon-cloud-upload icon  "></i>
                                                <span>Upload</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a ui-sref="flipboard">
                                                <i class="icon-list icon"></i>
                                                <span>Flipboard</span>
                                            </a>
                                        </li>
                                        <li class="m-b hidden-nav-xs"></li>
                                    </ul>
                                </nav>
                                <!-- / nav -->
                            </div>
                        </section>

                        <footer class="footer hidden-xs no-padder text-center-nav-xs">
                            <div class="bg hidden-xs ">
                            </div>
                        </footer>
                    </section>
                </aside>
                <!-- /.aside -->
                <section id="content">
                    
                    <section class="hbox stretch">

                        @RenderBody()
                    </section>

                        <a href="#" class="hide nav-off-screen-block" data-toggle="class:nav-off-screen" data-target="#nav"></a>
                    </section>
            </section>
        </section>

    </section>
                <!-- /Body content -->

                <script src="~/Scripts/webapp/jquery.min.js"></script>
                <!-- Bootstrap -->
                <script src="~/Scripts/webapp/bootstrap.min.js"></script>
                <!-- App -->
                <script src="~/Scripts/webapp/app.min.js"></script>
                <script src="~/Scripts/webapp/slimscroll/jquery.slimscroll.min.js"></script>
                <script src="~/Scripts/webapp/app.plugin.js"></script>
                <script type="text/javascript" src="~/Scripts/webapp/jPlayer/jquery.jplayer.min.js"></script>
                <script type="text/javascript" src="~/Scripts/webapp/jPlayer/add-on/jplayer.playlist.min.js"></script>
                <script type="text/javascript" src="~/Scripts/webapp/jPlayer/demo.js"></script>

                @RenderSection("scripts", required: false)

</body>
</html>
